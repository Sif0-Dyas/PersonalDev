# Bass Stage Client Environment Configuration
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:8000

# OAuth Configuration (if using client-side OAuth in addition to server-side)
# These would be used if implementing direct client-side OAuth flows
# REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# App Configuration
REACT_APP_APP_NAME=Bass Stage
REACT_APP_VERSION=1.0.0

# Environment
REACT_APP_ENVIRONMENT=development

# Feature Flags (Optional)
# REACT_APP_ENABLE_APPLE_LOGIN=false
# REACT_APP_ENABLE_ANALYTICS=false